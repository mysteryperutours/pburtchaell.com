<meta property=og:type content=website>
<meta property=og:title content="{{page.title}}">
<meta property=og:description content="{{#if page.description}}{{page.description}}{{else}}{{site.description}}{{/if}}">
<meta property=og:url content={{site.url}}>
<meta property=og:site_name content="{{site.title}}">
<meta property=og:profile:last_name content={{author.name.last}}>
<meta property=og:image content="{{#if featuredImage}}/assets/{{featuredImage}}{{else}}/assets/img/site/icon/og.png">
<meta property=og:image:type content=image/png>
<meta property=og:image:width content=1024>
<meta property=og:image:height content=1024>{{/if}}
