---
layout: none
---

<section class="portfolio-page portfolio-page--current" data-type=\{{project.type}}
  data-ng-animate-children=true
  data-ng-animate="{ enter: 'test-animation' }">

  	<template id=project-title>
  		<style>
  			:host {

  			}
  		</style>
  		<hrgroup class=project-page--hrgroup>
		    <h1 class=project-page--title>\{{project.title}}</h1>
        <h2 class=portfolio-page--description>
		  </hrgroup>
  	</template>

	<header class="portfolio-page--header" data-type=\{{project.type}}>
		<hrgroup class=portfolio-page--hrgroup>
		  <h1 class=portfolio-page--title>\{{project.title}}</h1>
			
		</hrgroup>
	</header>

	<section class=page-content>
		<div class=wrapper>
			\{{project.body}}
		</div>
	</section> 

	<aside>
		<div class=wrapper>
			<ul>
				<li>Type: \{{project.type}}
				<li>
				<li>
			</ul>  
		</div>
	</aside> 

	<footer class=portfolio-page--footer>

	</footer>

</section>

<section class="portfolio-page portfolio-page--next" data-ng-click="load()">
  <header>
    <i>Up next</i><br>
    <span>\{{project.next.title}}</span>
    <span>\{{project.next.desciption}}</span>
  </header>
</section>
