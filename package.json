  "scripts": {
    "postinstall": "brew install ImageMagick",
    "prebuild": "rm -rf ./dist && rm -rf ./app/images && `npm bin`/gulp images",
    "build": "NODE_ENV=production `npm bin`/webpack --config webpack.production.config.js",
    "predeploy:staging": "npm run build",
    "predeploy:production": "npm run build",
    "deploy:staging": "`npm bin`/gulp deploy:staging",
    "deploy:production": "`npm bin`/gulp deploy:production",
    "prestart": "rm -rf ./app/images && `npm bin`/gulp images",
    "start": "NODE_ENV=development node server.js"
  },
  "config": {
    "DEV_SERVER_PORT": "1996",
    "DEV_SERVER_ADDRESS": "0.0.0.0"
  },
