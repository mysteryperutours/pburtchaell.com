{
  "version": "4.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https+git://github.com/pburtchaell/pburtchaell.com.git"
  },
  "scripts": {
    "heroku-postbuild": "npm run build",
    "build": "`npm bin`/babel app --out-dir .tmp",
    "start-development": "nodemon app/server --exec `npm bin`/babel-node",
    "start-production": "node .tmp/server",
    "start": "if test \"$NODE_ENV\" = \"production\"; then npm run start-production; else npm run start-development; fi",
    "flow": "`npm bin`/flow; test $? -eq 0 -o $? -eq 2",
    "test": "npm run flow"
  },
  "config": {
    "PORT": "8000",
    "HOST": "0.0.0.0"
  },
  "engines": {
    "node": "6.5.0",
    "npm": "3.10.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.2",
    "classnames": "^2.2.5",
    "graphql": "^0.7.1",
    "history": "^4.3.0",
    "isomorphic-fetch": "^2.1.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "universal-router": "^1.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.33.0",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "postcss-calc": "^5.3.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-import": "^8.1.2",
