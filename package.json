{
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https+git://github.com/<USERNAME>/<REPOSITORY>.git"
  },
  "scripts": {
    "precommit": "echo 'Running pre-commit hook...' && npm test",
    "prebuild": "rm -rf ./build",
    "build": "NODE_ENV=production `npm bin`/webpack --progress",
    "flow": "`npm bin`/flow; test $? -eq 0 -o $? -eq 2",
    "generate": "./bin/generate.sh",
    "lint": "`npm bin`/eslint src/**/*.js test/**/*.js",
    "pretest": "npm run lint",
    "test": "./bin/test.sh",
    "start": "`npm bin`/webpack-dev-server --content-base app/ --open --port 8080 --history-api-fallback --inline --client-log-level warning"
  },
  "pre-commit": [
    "precommit"
  ],
  "jest": {
    "setupFiles": [
      "whatwg-fetch",
      "./test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "engines": {
    "node": "6.5.0",
    "npm": "3.10.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "jest-css-modules": "^1.1.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.1",
    "webpack-bundle-analyzer": "^2.2.0",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
