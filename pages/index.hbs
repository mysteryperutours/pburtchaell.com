---
title: Home
layout: none
---
{{> open-inc ~}}
{{> header-inc ~}}
<main role=main id=main>
  <section class=article-feed>

      {{#withSort post 'this.data.date'}}
        {{#each this.pages}}
        <article class=article-item>
          <header class=article-item-header>
            <a href="{{findURL this.dest 'dist/development' ''}}" rel=prefetch role=link class=line>
              <em><span class=article-item-title>{{this.data.title}}</span></em>
            </a>
            <small><time class=article-item-time>{{prettyDate this.data.date}}</time></small>
          </header>
        </article>
        {{/each ~}}
      {{/withSort ~}}

  </section><!-- /article-feed -->
</main>
{{> footer-inc ~}}
{{> close-inc ~}}
